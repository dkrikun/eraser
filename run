#! /bin/bash

$BOOST_ROOT/bjam eraser_inst variant=debug
javac src/eraser/Proxy.java -d bin
classpath=$eraser/inputs/inc
class=Inc
args="2 5 2"
rm -f dump out *.log backtrace stdout

( java -Dcom.sun.management.jmxremote  -agentpath:./bin/liberaser.so=--log-level=1 -Xbootclasspath/a:./bin -cp $classpath $class $args > stdout ) >& out
